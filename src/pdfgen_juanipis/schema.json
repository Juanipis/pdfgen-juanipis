{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "PDFGenJuanipisDocument",
  "type": "object",
  "additionalProperties": true,
  "properties": {
    "title": {
      "type": "string"
    },
    "refs_catalog": {
      "type": "object",
      "additionalProperties": {
        "type": "string"
      }
    },
    "theme": {
      "type": "object",
      "properties": {
        "header_banner_path": {
          "type": "string"
        },
        "header_banner_path_cont": {
          "type": "string"
        },
        "header_logo_path": {
          "type": "string"
        },
        "title_line1": {
          "type": "string"
        },
        "title_line2": {
          "type": "string"
        },
        "footer_site": {
          "type": "string"
        },
        "footer_phone": {
          "type": "string"
        },
        "show_header_titles": {
          "type": "boolean"
        }
      },
      "additionalProperties": true
    },
    "sections": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/section"
      }
    },
    "pages": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/page"
      }
    }
  },
  "anyOf": [
    {
      "required": [
        "sections"
      ]
    },
    {
      "required": [
        "pages"
      ]
    }
  ],
  "definitions": {
    "section": {
      "type": "object",
      "properties": {
        "title": {
          "type": "string"
        },
        "subtitle": {
          "type": "string"
        },
        "content": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/block"
          }
        },
        "refs": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "footer_notes": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      },
      "required": [
        "content"
      ],
      "additionalProperties": true
    },
    "page": {
      "type": "object",
      "properties": {
        "blocks": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/block"
          }
        },
        "refs": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "footer_notes": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      },
      "required": [
        "blocks"
      ],
      "additionalProperties": true
    },
    "block": {
      "type": "object",
      "properties": {
        "type": {
          "type": "string",
          "enum": [
            "text",
            "table",
            "figure",
            "map_grid",
            "html"
          ]
        },
        "text": {
          "type": [
            "string",
            "array"
          ],
          "items": {
            "type": "string"
          }
        },
        "html": {
          "type": "string"
        },
        "refs": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "footer_notes": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "table": {
          "$ref": "#/definitions/table"
        },
        "path": {
          "type": "string"
        },
        "caption": {
          "type": "string"
        },
        "wide": {
          "type": "boolean"
        },
        "items": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "path": {
                "type": "string"
              },
              "label": {
                "type": "string"
              }
            },
            "required": [
              "path"
            ],
            "additionalProperties": true
          }
        },
        "source": {
          "type": "string"
        }
      },
      "required": [
        "type"
      ],
      "additionalProperties": true
    },
    "table": {
      "type": "object",
      "properties": {
        "groups": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "title": {
                "type": "string"
              },
              "months": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              }
            },
            "required": [
              "title",
              "months"
            ],
            "additionalProperties": true
          }
        },
        "rows": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "dep": {
                "type": "string"
              },
              "vals": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              }
            },
            "required": [
              "dep",
              "vals"
            ],
            "additionalProperties": true
          }
        },
        "total_width": {
          "type": "number"
        },
        "dep_width": {
          "type": "number"
        },
        "show_header": {
          "type": "boolean"
        }
      },
      "required": [
        "groups",
        "rows"
      ],
      "additionalProperties": true
    }
  }
}